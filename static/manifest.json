{
  "name": "Youtube Suspender: Save bandwidth for background videos",
  "version": "0.1",
  "web_accessible_resources": ["ctx.js"],
  "description": "Lowers video quality for background videos to save bandwidth.",
  "manifest_version": 2,
  "permissions": ["storage"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["https://www.youtube.com/*"],
    "js": ["contentScript.js"]
  }],
  "icons": { 
    "128": "icon128.png" 
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}